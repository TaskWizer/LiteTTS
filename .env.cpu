# CPU-only deployment configuration
ENABLE_GPU=false
LITETTS_DEVICE=cpu

# Performance optimization for CPU (matching local settings.json)
MAX_MEMORY_MB=4096
TARGET_RTF=0.25
LITETTS_WORKERS=4

# Dynamic CPU allocation settings (from config/settings.json)
DYNAMIC_CPU_ALLOCATION_ENABLED=true
CPU_TARGET=75.0
AGGRESSIVE_MODE=true
THERMAL_PROTECTION=true
ONNX_INTEGRATION=true
UPDATE_ENVIRONMENT=true

# CPU-specific optimizations
OMP_NUM_THREADS=8
MKL_NUM_THREADS=8
OPENBLAS_NUM_THREADS=8
VECLIB_MAXIMUM_THREADS=8

# ONNX Runtime performance settings
ORT_DISABLE_ALL_OPTIMIZATION=0
ORT_ENABLE_CPU_FP16_OPS=1
ORT_GRAPH_OPTIMIZATION_LEVEL=all
ORT_EXECUTION_MODE=parallel
ORT_ENABLE_MEM_PATTERN=1
ORT_ENABLE_CPU_MEM_ARENA=1
ORT_ENABLE_MEM_REUSE=1

# Disable GPU-related environment variables
CUDA_VISIBLE_DEVICES=
NVIDIA_VISIBLE_DEVICES=
GPU_RUNTIME=
GPU_DEVICES=

# CPU optimization flags
ONNX_DISABLE_ALL_OPTIMIZATION=0
ONNX_ENABLE_CPU_FP16_OPS=1
ONNX_TENSORRT_MAX_WORKSPACE_SIZE=

# Memory allocation optimizations
MALLOC_ARENA_MAX=4
MALLOC_MMAP_THRESHOLD_=131072
MALLOC_TRIM_THRESHOLD_=131072
